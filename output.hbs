<div style="
    font-family: 'Helvetica Neue',Helvetica,Arial,sans-serif;
    color: #484848;
">
    <div style="
            background-image: url({{image}});
            background-size: cover;
            background-position: 50% 50%;
            color: #fff;
            height: 200px;
            position: relative;
            ">
        <div style="
    background-color: #2d2d2d;
    font-size: 24px;
    display: inline-block;
    padding: 5px 15px;
    position: absolute;
    left: 0;
    bottom: 20px;
    ">
            <span>{{price}}</span>
            <sup style="font-size: 14px">{{currency}}</sup>
        </div>
        <img alt="AirBnB Logo"
             style="float: right"
             src="data:image/png;base64,"/>
    </div>
    <div>
        <div>
            <div style="font-size: 24px;">
                {{title}}
            </div>
            <div style="font-size: 14px;">
                {{type}}
                {{#if rating}}
                    Â· {{rating}} <span style="color: #ffb400">&#9733;</span>&nbsp;
                {{/if}}
                {{#if reviews}}
                    ({{reviews}} reviews)
                {{/if}}
            </div>
            <div style="font-size: 14px;">
                {{city}}, {{state}}, {{country}}
            </div>
        </div>
        <div>
            <p>{{description}}</p>
        </div>
    </div>
</div>

